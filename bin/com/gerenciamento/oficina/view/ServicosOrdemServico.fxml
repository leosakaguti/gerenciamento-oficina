      <Button fx:id="btnAdicionar" layoutX="145.0" layoutY="330.0" mnemonicParsing="false" text="Adicionar" />
      <Button fx:id="btnRemover" layoutX="72.0" layoutY="330.0" mnemonicParsing="false" text="Remover" />
      <Button fx:id="btnVoltar" layoutX="14.0" layoutY="330.0" mnemonicParsing="false" onAction="#onClickBtnVoltar" text="Voltar" />
