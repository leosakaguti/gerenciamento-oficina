      <Button fx:id="btnAdicionar" layoutX="109.0" layoutY="66.0" mnemonicParsing="false" text="Adicionar" />
      <Button fx:id="btnRemover" layoutX="26.0" layoutY="66.0" mnemonicParsing="false" text="Remover" />
